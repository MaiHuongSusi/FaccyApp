<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:width="65dp" android:height="65dp">
        <shape android:shape="oval">
            <solid android:color="#860000" />
        </shape>
    </item>

    <item
        android:width="60dp"
        android:height="60dp"
        android:bottom="2.5dp"
        android:left="2.5dp"
        android:right="2.5dp"
        android:top="2.5dp">
        <shape android:shape="oval">
            <solid android:color="#D6D4D4" />
        </shape>
    </item>

    <item
        android:width="25dp"
        android:height="25dp"
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp">
        <shape android:shape="rectangle">
            <solid android:color="#860000" />
        </shape>
    </item>

</layer-list>